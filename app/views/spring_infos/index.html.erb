<h1>Listing spring_infos</h1>
<h3>CSVアップロード</h3>
<div class="alert.alert-success">
  <%= form_tag import_csv_spring_infos_path, action: "import_csv", method: "post",multipart: true do %>
    <div class="field">
      <%= file_field_tag :csv_file %>
      <br />
    </div>
    <div class="actions">
      <%= submit_tag "CSVファイル取込" %>
    </div>
  <% end %>
</div>


<table class="table-bordered">
  <thead>
    <tr>
      <th>温泉地</th>
      <th>泉質</th>
      <th>泉質（詳）</th>
      <th>泉質（旧）</th>
      <th>泉温</th>
      <th>pH</th>
      <th>浸透圧</th>
      <th>湧出量</th>
      <th>メタケイ酸</th>
      <th>備考</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @spring_infos.each do |spring_info| %>
      <tr>
        <td><%= spring_info.spring_place_str %></td>
        <td><%= spring_info.category_str %></td>
        <td><%= spring_info.quality_name %></td>
        <td><%= spring_info.old_quality_name %></td>
        <td><%= spring_info.temperature %></td>
        <td><%= spring_info.pH %></td>
        <td><%= spring_info.osmotic_str %></td>
        <td><%= spring_info.discharge %></td>
        <td><%= spring_info.metasilicic_acid %></td>
        <td><%= spring_info.remarks %></td>
        <td><%= link_to 'Edit', edit_spring_info_path(spring_info) %></td>
        <td><%= link_to 'Del', spring_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Spring info', new_spring_info_path %>
